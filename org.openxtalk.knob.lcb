public handler OnMouseUp()
   put nothing into mPrevMouseH
   put nothing into mPrevMouseV
   put false into mMouseDown
	if the mouse position is within my bounds then
		post "mouseUp" with [the click button] -- to my script object
	else
		post "mouseRelease" with [the click button]
	end if
	redraw all
end handler

private variable mPrevMouseH as optional Number
private variable mPrevMouseV as optional Number
private variable mMouseDown as optional Boolean

public handler OnMouseMove()
   if mMouseDown is not nothing and mMouseDown is true then
      variable tMouseH as Number
      variable tMouseV as Number
      variable tMouseLoc as List
      variable tDiff as Number
      execute script "return mouseLoc()"
      split the result by "," into tMouseLoc
      parse tMouseLoc as list of number into tMouseLoc
      put tMouseLoc[1] into tMouseH
      put tMouseLoc[2] into tMouseV
      -- put the x of the mouse position / my pixel scale into tMouseH
      -- put the y of the mouse position / my pixel scale into tMouseV
      if mPrevMouseH < tMouseH then -- moving right
         --log "right"
         put tMouseH - mPrevMouseH into tDiff
         add tDiff to mCurrentRotation
      else if mPrevMouseH > tMouseH then -- moving left
         --log "left"
         put mPrevMouseH - tMouseH into tDiff
         subtract tDiff from mCurrentRotation
      end if
      if tMouseV < mPrevMouseV then -- moving up
         --log "up"
         put mPrevMouseV - tMouseV into tDiff
         add tDiff to mCurrentRotation
      else if tMouseV > mPrevMouseV then -- moving down
         --log "down"
         put tMouseV - mPrevMouseV into tDiff
         subtract tDiff from mCurrentRotation
      end if
      if mCurrentRotation > mMaxRotatiom then
         put mMaxRotatiom into mCurrentRotation
      end if
      if mCurrentRotation < 0 then
         put 0 into mCurrentRotation
      end if
      variable tDivisor as Number
      variable tPercentage as Number
      -- put trunc( 1.27 * ( lRotate / 2.9) ) into fld 1
      put mMaxRotatiom / 100 into tDivisor
      put mCurrentRotation / tDivisor into tPercentage
      -- log [tPercentage]
      -- calculateRotatedPath(mWorkingPath, mCurrentRotation, mCenter, true)
      -- put preparePathForRotation(mSVGCircleBase, mIconOrigin, rectangle [0,0,mIconSize,mIconSize], my rectangle) into mCenter
      -- put preparePathForRotation(mWorkingPath, mIconOrigin, rectangle [0,0,mIconSize,mIconSize], my rectangle) into mCenter
      setRotation(mCurrentRotation)
      put tMouseH into mPrevMouseH
      put tMouseV into mPrevMouseV
      redraw all
      post "mouseMove" to my script object with [tMouseH,tMouseV] -- to my script object
      post "knobTurned" to my script object with [tPercentage] -- to my script object
   end if
end handler

