property iconSVGPath        get getIconPath          set setIconPath
metadata iconSVGPath.editor is "com.livecode.pi.text"
private handler getIconPath() returns String
	return the instructions of mSVGPath
end handler
private handler setIconPath(in pPath as String)
	put path pPath into mSVGPath
	put "custom" into mIconStyle
	initializationCalculations()
	redraw all
end handler

private variable mSVGPathColor as Color
property SVGPathColor        	 get getSVGPathColor             set setSVGPathColor
metadata SVGPathColor.editor is "com.livecode.pi.color"
metadata SVGPathColor.section is "Colors"
metadata SVGPathColor.label is "SVG Path color"
private handler getSVGPathColor() returns String
	return colorToString(mSVGPathColor, false)
end handler
private handler setSVGPathColor(in pColor as String)
	put stringToColor(pColor) into mSVGPathColor
	redraw all
end handler

private variable mIconSize          as Number
property iconSize           get mIconSize            set setIconSize
private handler setIconSize(in pSize as Number)
	put pSize into mIconSize
	initializationCalculations()
	redraw all
end handler

private variable mMaxRotatiom   as Number
property maxRotation    get mMaxRotatiom		     set mMaxRotatiom

private variable mCurrentRotation   as Number
property currentRotation    get mCurrentRotation		     set setRotation
private handler setRotation(in pRotation as Number)
	initializationCalculations()
	calculateRotatedPath(mWorkingPath, pRotation, mCenter, true)
   calculateRotatedPath(mWorkingBasePath, pRotation, mCenter, true)
   put pRotation into mCurrentRotation
	redraw all
end handler

